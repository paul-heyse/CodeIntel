{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/symbol_use_edges.json",
  "title": "Symbol Use Edges Export",
  "allOf": [
    { "$ref": "./base.json" },
    {
      "type": "object",
      "properties": {
        "symbol": { "type": "string" },
        "def_path": { "type": "string" },
        "use_path": { "type": "string" },
        "same_file": { "type": "boolean" },
        "same_module": { "type": "boolean" }
      },
      "required": ["symbol", "def_path", "use_path"]
    }
  ]
}
