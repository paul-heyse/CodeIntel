{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/test_coverage_edges.json",
  "title": "Test coverage edges export",
  "type": "object",
  "allOf": [{ "$ref": "base.json" }],
  "properties": {
    "repo": { "type": "string" },
    "commit": { "type": "string" },
    "test_id": { "type": ["string", "null"] },
    "test_goid_h128": { "type": ["integer", "null"] },
    "function_goid_h128": { "type": ["integer", "null"] },
    "urn": { "type": ["string", "null"] },
    "rel_path": { "type": ["string", "null"] },
    "qualname": { "type": ["string", "null"] },
    "covered_lines": { "type": ["integer", "null"] },
    "executable_lines": { "type": ["integer", "null"] },
    "coverage_ratio": { "type": ["number", "null"] },
    "last_status": { "type": ["string", "null"] },
    "created_at": { "type": ["string", "null"], "format": "date-time" }
  },
  "required": ["repo", "commit"],
  "additionalProperties": true
}
