{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/test_coverage_edges.json",
  "title": "Test Coverage Edges Export",
  "allOf": [
    { "$ref": "./base.json" },
    {
      "type": "object",
      "properties": {
        "test_id": { "type": "string" },
        "function_goid_h128": { "type": ["integer", "string"] },
        "rel_path": { "type": "string" },
        "covered_lines": { "type": "number" },
        "executable_lines": { "type": "number" },
        "coverage_ratio": { "type": "number" }
      },
      "required": ["test_id", "function_goid_h128", "rel_path"]
    }
  ]
}
