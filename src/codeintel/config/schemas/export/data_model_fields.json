{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/data_model_fields.json",
  "title": "Data model fields export",
  "type": "object",
  "allOf": [{ "$ref": "base.json" }],
  "properties": {
    "repo": { "type": "string" },
    "commit": { "type": "string" },
    "model_id": { "type": "string" },
    "field_name": { "type": "string" },
    "field_type": { "type": ["string", "null"] },
    "required": { "type": "boolean" },
    "has_default": { "type": "boolean" },
    "default_expr": { "type": ["string", "null"] },
    "constraints_json": {},
    "source": { "type": "string" },
    "rel_path": { "type": "string" },
    "lineno": { "type": ["integer", "null"] },
    "created_at": { "type": ["string", "null"], "format": "date-time" }
  },
  "required": [
    "repo",
    "commit",
    "model_id",
    "field_name",
    "required",
    "has_default",
    "constraints_json",
    "source",
    "rel_path"
  ],
  "additionalProperties": true
}
