{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/data_model_relationships.json",
  "title": "Data model relationships export",
  "type": "object",
  "allOf": [{ "$ref": "base.json" }],
  "properties": {
    "repo": { "type": "string" },
    "commit": { "type": "string" },
    "source_model_id": { "type": "string" },
    "target_model_id": { "type": "string" },
    "target_module": { "type": ["string", "null"] },
    "target_model_name": { "type": ["string", "null"] },
    "field_name": { "type": "string" },
    "relationship_kind": { "type": "string" },
    "multiplicity": { "type": ["string", "null"] },
    "via": { "type": ["string", "null"] },
    "evidence_json": {},
    "rel_path": { "type": "string" },
    "lineno": { "type": ["integer", "null"] },
    "created_at": { "type": ["string", "null"], "format": "date-time" }
  },
  "required": [
    "repo",
    "commit",
    "source_model_id",
    "target_model_id",
    "field_name",
    "relationship_kind",
    "rel_path"
  ],
  "additionalProperties": true
}
