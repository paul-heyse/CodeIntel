{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/behavioral_coverage.json",
  "title": "Behavioral coverage export",
  "type": "object",
  "allOf": [{ "$ref": "base.json" }],
  "properties": {
    "repo": { "type": "string" },
    "commit": { "type": "string" },
    "test_id": { "type": "string" },
    "test_goid_h128": { "type": ["integer", "null"] },
    "rel_path": { "type": "string" },
    "qualname": { "type": ["string", "null"] },
    "behavior_tags": {},
    "tag_source": { "type": "string" },
    "heuristic_version": { "type": ["string", "null"] },
    "llm_model": { "type": ["string", "null"] },
    "llm_run_id": { "type": ["string", "null"] },
    "created_at": { "type": ["string", "null"], "format": "date-time" }
  },
  "required": ["repo", "commit", "test_id", "rel_path", "behavior_tags", "tag_source"],
  "additionalProperties": true
}
