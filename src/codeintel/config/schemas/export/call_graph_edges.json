{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.codeintel.dev/export/call_graph_edges.json",
  "title": "Call graph edges export",
  "type": "object",
  "allOf": [{ "$ref": "base.json" }],
  "properties": {
    "repo": { "type": "string" },
    "commit": { "type": "string" },
    "caller_goid_h128": { "type": "integer" },
    "callee_goid_h128": { "type": ["integer", "null"] },
    "callsite_path": { "type": "string" },
    "callsite_line": { "type": ["integer", "null"] },
    "callsite_col": { "type": ["integer", "null"] },
    "language": { "type": "string" },
    "kind": { "type": ["string", "null"] },
    "resolved_via": { "type": ["string", "null"] },
    "confidence": { "type": ["number", "null"] },
    "evidence_json": {}
  },
  "required": [
    "repo",
    "commit",
    "caller_goid_h128",
    "callsite_path",
    "language"
  ],
  "additionalProperties": true
}
